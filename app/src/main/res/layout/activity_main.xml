<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/hello"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <Button
        android:id="@+id/btnnfoods"
        android:layout_width="127dp"
        android:layout_height="130dp"
        android:layout_marginStart="128dp"
        android:layout_marginLeft="128dp"
        android:layout_marginTop="112dp"
        android:background="#51192C"
        android:onClick="food"
        android:text="food"
        app:backgroundTint="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="OnClick" />

    <Button
        android:id="@+id/btnnlap"
        android:layout_width="127dp"
        android:layout_height="130dp"
        android:layout_marginStart="128dp"
        android:layout_marginLeft="128dp"
        android:layout_marginTop="24dp"
        android:background="#D5D5D5"
        android:onClick="laptop"
        android:text="laptop"
        app:backgroundTint="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnnfoods"
        tools:ignore="OnClick" />

    <Button
        android:id="@+id/btnnsamsung"
        android:layout_width="127dp"
        android:layout_height="130dp"
        android:layout_marginStart="128dp"
        android:layout_marginLeft="128dp"
        android:layout_marginTop="44dp"
        android:background="#51192C"
        android:onClick="Samsung"
        android:text="Samsung"
        app:backgroundTint="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnnlap"
        tools:ignore="OnClick" />

</androidx.constraintlayout.widget.ConstraintLayout>